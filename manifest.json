{
  "name": "WebsiteVisitHours",
  "short_name": "WebsiteVisitHours",
  "description": "Mark website if it is outside of visit hours",
  "version": "1.0.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
      "default_title": "URLColors",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
